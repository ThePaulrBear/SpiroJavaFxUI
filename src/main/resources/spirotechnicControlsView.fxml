<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import paul.wintz.javafx.widgets.EventButton?>
<?import paul.wintz.javafx.widgets.IntegerSpinner?>

<VBox alignment="TOP_CENTER" focusTraversable="true" prefWidth="300.0" spacing="20" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="paul.wintz.parametricequationdrawer.controllers.javafx.SpirotechnicControlsView">

    <IntegerSpinner fx:id="circleCount" />
    <EventButton fx:id="randomize" text="Randomize" />
    <EventButton fx:id="nextGraph" text="Load Next Graph" />
    <EventButton fx:id="previousGraph" text="Load Previous Graph" />
    <VBox fx:id="circlesColumn" maxHeight="20" spacing="10" />
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>

</VBox>
